{
    "family": "enterprise-document-chat-td",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::668015627697:role/ecsTaskExecutionRole",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "8192",
    "containerDefinitions": [
    {
        "name": "enterprise-document-chat-container",
        "image": "668015627697.dkr.ecr.us-east-2.amazonaws.com/enterprise-document-chat",
        "cpu": 1024,
        "essential": true,
        "portMappings": [
        {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp",
            "name": "enterprise-document-chat-container-8080-tcp",
            "appProtocol": "http"
        }
        ],
        "environment": [
        {
            "name": "LANGCHAIN_PROJECT",
            "value": "ENTERPRISE DOCUMENT CHAT"
        }
        ],
        "secrets": [
        {
            "name": "GROQ_API_KEY",
            "valueFrom": "arn:aws:secretsmanager:us-east-2:668015627697:secret:apikeys-9bXo4F:GROQ_API_KEY"
        },
        
        {
            "name": "GOOGLE_API_KEY",
            "valueFrom": "arn:aws:secretsmanager:us-east-2:668015627697:secret:apikeys-9bXo4F:GOOGLE_API_KEY"
        }
        
        ],
        "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "/ecs/enterprise-document-chat-td",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "ecs",
            "mode": "non-blocking",
            "max-buffer-size": "25m"
        }
        }
    }
    ]
}